---
import MainLayout from "../layouts/MainLayout.astro";
import Button from '../components/Button.astro';
import Link from '../components/Link.astro';
import { Image } from 'astro:assets';
import thumbsup from "../assets/images/thumbsup.png";
import ArtistCard from "../components/ArtistCard.astro";
import WhatIsPixel from "../components/WhatIsPixel.astro";
import viola_rama from "../assets/images/viola_rama.png";
import bradon_morse from "../assets/images/bradon_morse.png";
import danielle from "../assets/images/danielle.png";
import Billedegalleri from "../components/Billedegalleri.astro";
import Buyticketsheader from "../components/Buyticketsheader.astro";
import ApartofPixel from "../components/ApartofPixel.astro";
import Newsletter from "../components/Newsletter.astro";
import Billettyper from "../components/Billettyper.astro";
import Logo from "../components/Logo.astro";
import Pixel from "../components/Pixel.astro";
import Ticketform from "../components/Ticketform.astro";
---

<MainLayout title="Buy Tickets">

  <Logo />

  <Buyticketsheader id="herosection" />
  <Pixel />
  <Billettyper />

  <section id="ticketform" class="flex justify-center items-center mt-30 lg:mt-50">
    <h2>BUY YOUR TICKET</h2>
  </section>

  <Ticketform />

</MainLayout>

<style>
  @import url("https://use.typekit.net/ctz4zoo.css");
</style>
